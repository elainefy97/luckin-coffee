/* pages/component/swiper.wxss */
.containerSwiper {
  width: 100%;
  display: inline-block;


}

page {
  background-color: #F8F8F8;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
}

.page-body {
  padding-top: 0rpx;
}

.page-section {
  width: 100%;
  margin-bottom: 60rpx;
}

.page-section:last-child {
  margin-bottom: 0;
}

.page-section-spacing {
  box-sizing: border-box;
    /* padding: 0 10rpx; */
}

.page-section-title {
  font-size: 28rpx;
  color: #999999;
  margin-bottom: 10rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.page-section-title {
  padding-left: 0;
  padding-right: 0;
  padding: 0;
  margin-top: 60rpx;
  position: relative;
}

.swiper-item{
  display: block;
  height: 150px;
  box-sizing:border-box;
  border-radius:20rpx;
  overflow:hidden ;
}
.swiper-item-menu{
  height: 150px;
}
.spaceSiwper{
  margin-left: 10rpx;
  margin-right: 10rpx;
}

